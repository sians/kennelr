<%= form_tag kennels_path, method: :get do %>
  <div class="nav-search">
    <%= text_field_tag :query,
      params[:query],
      placeholder: 'search',
      class: "form-control address-input" %>

    <%= button_tag :submit, class: 'btn-icon' do %>
      <i class="fas fa-search"></i>
    <% end %>
  </div>
<% end %>
